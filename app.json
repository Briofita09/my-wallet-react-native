{
  "expo": {
    "name": "my-wallet-client",
    "slug": "my-wallet-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/my_wallet_logo_big.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/my_wallet_logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#8C11BE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
